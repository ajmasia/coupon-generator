{
  "name": "coupongenerator",
  "version": "0.1.2",
  "description": "Coupon generator app",
  "main": "app.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "webpack --mode production",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint --fix -c tslint.json 'src/**/*.{ts,tsx}'",
    "docker:install": "docker-compose run --rm --no-deps app npm i",
    "docker:uninstall": "docker-compose run --rm --no-deps app npm uninstall",
    "docker:lint": "docker-compose run --rm --no-deps app npm run lint",
    "docker:lint:fix": "docker-compose run --rm --no-deps app npm run lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajmasia/couponGenerator.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "webpack"
  ],
  "author": "Antonio José Masiá <email@antoniomasia.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajmasia/couponGenerator/issues"
  },
  "homepage": "https://github.com/ajmasia/couponGenerator#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "babel-loader": "^8.0.6",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.18.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@types/ramda": "^0.26.18",
    "ramda": "^0.26.1"
  }
}
